/* detail.wxss */
@import '../../../utils/font/iconfont.wxss';
page{
  background-color:#efeff4;
}
.scene_view{
  width:100%;
  height:200px !important;
}
.detail_head{
  width:100%;
  height:150px;
}
.sence_tip{
  width:100%;
  height:20px;
  position: absolute;
  margin-top:-30px;
}
.sence_tip1{
  width:65%;
  height:100%;
  color: #FFFFFF;
  line-height:20px;
  font-size: medium;
  text-indent:10px;
  float: left;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.sence_tip2{
  width:35%;
  height:100%;
  color: #FFFFFF;
  line-height:20px;
  font-size: medium;
  float: left;
  text-align: right;
}
.view_intr{
  width:100%;
  height:40px;
  margin-top:50px;
  background-color: #FFFFFF;

}
.view_icon{
  margin-top:10px;
  height:20px;
  width:20px;
  margin-left:20rpx;
  float: left;
}
.view_intr_text{
  height:40px;
  line-height:40px;
  color: #302f35;
  float: left;
  text-indent:10px;
}
