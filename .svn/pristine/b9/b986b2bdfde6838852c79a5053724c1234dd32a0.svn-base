  margin-top:-90rpx;
  text-align: right;
  margin-left:57%;
  position: absolute;
}
.blank{
  width:100%;
}
.detail_item{
  
}

.detail_price2{
  width:37%;
  height:40rpx;
  font-size:30rpx;
  color: #FFFFFF ;
  margin-top:-135rpx;
  margin-left:57%;
  position: absolute;
}
.detail_price_1{
  height:100%;
  float: right;
  font-size:smaller;
  line-height: 40rpx;
}
.detail_price_2{
  height:100%;
  float: right;
  color: #ffea00 ;
  font-size: medium;
  line-height: 40rpx;
}

.detail_quan{
  width:95%;
  margin-left:2.5%;
  height:205rpx;
  margin-top:20rpx;
}
.quan_left{
  width:70%;
  float: left;
  height:100%;
  background-color: #FFFFFF;
}
.quan_right{
  width:30%;
