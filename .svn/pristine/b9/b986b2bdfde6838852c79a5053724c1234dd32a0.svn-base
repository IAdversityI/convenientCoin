  padding-top:25rpx;
  padding-right:10rpx;
  color: #d2d3d5;
}
.context{
  width: 90%;
  color:#666666;
  font-size:9pt; 
  line-height:37rpx;
  margin-left: 3%;
  margin-top: 11rpx;
}
.head{
  width: 100%;
  height:70rpx;
  line-height:70rpx;
  font-weight: bold;
  text-indent: 25rpx;
}
.options{
  width:100%;
  height:153rpx;
  margin-top:99rpx;
  background-color: #ffffff;
}
.options .option{
  height:100%;
  width:33%;
  text-align: center;
  float: left;

}
.options .line{
  width:1rpx;
  height:115rpx;
  margin-top:19rpx;
  float:left;
}

.options .option .icon{
  width:62rpx;
  height:61rpx;
  margin-top:25rpx;
}
.options .option .lable{
  font-size:23rpx;
  color: #666666;
}