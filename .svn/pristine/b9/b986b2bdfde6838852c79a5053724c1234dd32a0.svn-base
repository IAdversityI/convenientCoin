  text-align: center;
  line-height:50rpx;
  margin-top:-139rpx;
}
.quan_right_buy{
  position: absolute;
  width:93rpx;
  height:31rpx;
  margin-left:69rpx;
  color: #ff9900;
  text-align: center;
  line-height:31rpx;
  margin-top:-77rpx;
  font-size:16rpx;
  background-color:#FFFFFF;
  border:4rpx #FFFFFF solid;
  border-radius:2pt 2pt 2pt 2pt;
}
.mask{
  width:100%;
  background-color: #000000;
  opacity: 0.5;
  z-index:99;
  position: fixed;
  
}
.tips{
  width:74%;
  margin-left:16%;
  background-color:  #FFFFFF;
  height: 291rpx;
  position: fixed;
  margin-top:314rpx;
  z-index: 999;
  border-radius:5rpx 5rpx 5rpx 5rpx;
}
.tips_title{
  width:100%;
  height:30%;
  font-size:14pt;
  line-height: 87rpx;
  text-align: center;
}
.tips_title2{
  width:80%;
  margin-left:10%;
  height:30%;
  font-size:10pt;
  line-height:46rpx;
  color: #999999;
  text-align: center;
}
.tips_btn{
  width:49%;
  height:30%;
  float: left;
  margin-top:5%;
  border-top: 1px #d2d3d5 solid; 
  line-height: 87rpx;
  text-align: center;
}
.nodata{
  max-width:60%;
  margin:0rpx 20%;
}
.timer{
  font-size:15rpx;
  margin-top:-49rpx;
}
.quan_time{
  width:300rpx;
  height:30rpx;
  font-size:6pt;
  float: left;
